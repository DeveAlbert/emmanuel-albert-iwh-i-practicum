doctype html
meta(charset="utf-8")
html(lang="en")
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a(href='/update-cobj') Add/Update this table
        table
            thead 
                th Book Author 
                th Book Genre 
                th Book ISSN
                th Book Title  
            each favoriteBook in data
                tr 
                    td #{favoriteBook.properties.book_author}
                    td #{favoriteBook.properties.book_genre}
                    td #{favoriteBook.properties.book_issn}
                    td #{favoriteBook.properties.book_title}